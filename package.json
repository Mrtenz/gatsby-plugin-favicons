{
  "name": "gatsby-plugin-favicons",
  "version": "1.0.4",
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "engines": {
    "node": ">=10"
  },
  "description": "Generate favicons for your Gatsby website",
  "main": "index.js",
  "repository": "https://github.com/Mrtenz/gatsby-plugin-favicons.git",
  "author": "Maarten Zuidhoorn <maarten@zuidhoorn.com>",
  "license": "MIT",
  "dependencies": {
    "favicons-webpack-plugin": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.21.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react": "^16.8.0"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "webpack": "^4.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir .",
    "prepublish": "yarn run build",
    "prettier:diff": "prettier --write --config ./.prettierrc --list-different 'src/**/*.js'"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --config ./.prettierrc --config-precedence file-override",
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
